

while True:
    tabu = int(input('Digite um numero para descobrir sua tabuada: '))

    if tabu <= 0:
        print('Encerrado.')
        break
    print(f'TABELA DE {tabu}')
    for i in range(1,11):
        print(f'A tabuada de {tabu} x {i} =  {tabu * i}')
        print()
print(f'Resultado Ã© {tabu}')



